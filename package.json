{
	"name": "jacob-dental-group",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/DopamineDriven/jacob-dental-group.git",
	"author": "Andrew Ross <andrew.simpson.ross@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "next -p 3443",
		"build": "next build",
		"start": "next start -p 4334",
		"analyze": "ANALYZE=true yarn build",
		"tsconfig:effective": "tsc --showConfig",
		"postinstall": "npx patch-package",
		"client:codegen": "yarn apollo codegen:generate --localSchemaFile=graphql.schema.json --target=typescript --includes=graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=types/graphql-global-types.ts",
		"codegen:generate": "graphql-codegen --config codegen.yml"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn tsc && pretty-quick --staged && yarn tsconfig:effective"
		}
	},
	"dependencies": {
		"@apollo/client": "^3.2.5",
		"@artsy/fresnel": "^1.3.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-brands-svg-icons": "^5.15.1",
		"@fortawesome/free-regular-svg-icons": "^5.15.1",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@fortawesome/react-fontawesome": "^0.1.12",
		"@types/graphql": "^14.5.0",
		"apollo": "^2.31.1",
		"classnames": "^2.2.6",
		"graphql": "^15.4.0",
		"next": "^10.0.1",
		"patch-package": "^6.2.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^3.0.0",
		"@graphql-codegen/cli": "^1.19.1",
		"@graphql-codegen/introspection": "^1.18.0",
		"@graphql-codegen/typed-document-node": "^1.17.10",
		"@graphql-codegen/typescript-apollo-client-helpers": "^1.1.0",
		"@graphql-codegen/typescript-document-nodes": "^1.17.8",
		"@graphql-codegen/typescript-graphql-files-modules": "^1.18.0",
		"@graphql-codegen/typescript-operations": "^1.17.9",
		"@graphql-codegen/typescript-react-apollo": "^2.1.1",
		"@graphql-typed-document-node/core": "^3.1.0",
		"@graphql-typed-document-node/patch-cli": "^3.0.8",
		"@headlessui/react": "^0.2.0",
		"@types/classnames": "^2.2.11",
		"@types/gtag.js": "^0.0.3",
		"@types/node": "^14.14.6",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"autoprefixer": "9.8.6",
		"husky": "^4.3.0",
		"postcss-flexbugs-fixes": "^5.0.0",
		"postcss-import": "12.0.0",
		"postcss-nesting": "^7.0.1",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"stylelint": "^13.7.2",
		"stylelint-config-recommended": "^3.0.0",
		"tailwindcss": "^1.9.6",
		"typescript": "^4.0.5"
	}
}
